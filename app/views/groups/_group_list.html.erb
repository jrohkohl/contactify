<div id = "contact_dd_<%=dom_id(group)%>" class= "mt-3" style = "display: none;">

  <% group.contacts.by_created_date.each do |contact| %>

    <div class = "d-flex justify-content-between align-items-center">

      <div class = "small"><%= contact.name %></div>

      <div class="small"><%= contact.preferred_method %></div>

    </div>

  <% end %>

</div>