<p id="notice"><%= notice %></p>

<h1>Contacts</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Image</th>
      <th>Organization</th>
      <th>Preferred method</th>
      <th>Personal number</th>
      <th>Work number</th>
      <th>Email</th>
      <th>Instagram url</th>
      <th>Facebook url</th>
      <th>Owner</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contacts.each do |contact| %>
      <tr>
        <td><%= contact.name %></td>
        <td><%= contact.image %></td>
        <td><%= contact.organization %></td>
        <td><%= contact.preferred_method %></td>
        <td><%= contact.personal_number %></td>
        <td><%= contact.work_number %></td>
        <td><%= contact.email %></td>
        <td><%= contact.instagram_url %></td>
        <td><%= contact.facebook_url %></td>
        <td><%= contact.owner_id %></td>
        <td><%= link_to 'Show', contact %></td>
        <td><%= link_to 'Edit', edit_contact_path(contact) %></td>
        <td><%= link_to 'Destroy', contact, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contact', new_contact_path %>
